	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/econominhas/backend.git"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.456.0",
		"@aws-sdk/client-ses": "^3.454.0",
		"@nestjs/common": "^10.2.10",
		"@nestjs/core": "^10.2.10",
		"@nestjs/platform-express": "^10.2.10",
		"@prisma/client": "5.6.0",
		"@techmmunity/utils": "^1.10.1",
		"change-case": "^5.2.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"jsonwebtoken": "^9.0.2",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.8.1",
		"semver": "^7.5.4",
		"uid": "^2.0.2"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.2.1",
		"@nestjs/schematics": "^10.0.3",
		"@nestjs/testing": "^10.2.10",
		"@redocly/cli": "^1.4.1",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.10",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/node": "^20.9.4",
		"@types/semver": "^7.5.6",
		"@types/supertest": "^2.0.16",
		"dbdocs": "^0.8.4",
		"env-cmd": "^10.1.0",
		"eslint": "^8.54.0",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"lint-staged": "^15.1.0",
		"prettier": "^3.1.0",
		"prisma": "^5.6.0",
		"prisma-case-format": "^2.0.1",
		"prisma-dbml-generator": "^0.10.0",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.1",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.3.2"
	},
	"scripts": {
		"db:prisma": "prisma generate",
		"db:docs": "prisma generate --generator dbml && dbdocs build ./prisma/schema.dbml",
		"openapi:serve": "redocly preview-docs",
		"openapi:build": "redocly bundle -o dist/bundle.yaml",
		"prepare": "husky install",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:docker": "nest start --watch",
		"start:dev": "docker compose up",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"clean:docker": "docker container rm econominhas_api && docker image rm backend-api",
		"lint:ts": "tsc --project tsconfig.lint.json",
		"lint:code": "eslint \"src/**/*.ts\" --fix --quiet",
		"lint:prisma": "prisma-case-format --file prisma/schema.prisma --map-table-case snake,plural --map-field-case snake --map-enum-case snake",
		"lint:openapi": "redocly lint",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	}
}
